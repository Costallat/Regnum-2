	inventory_image = "ruby_ruby_pick.png",
	tool_capabilities = {
		full_punch_interval = 0,
		max_drop_level=3,
		groupcaps={
			cracky={times={[1]=2.0, [2]=1.0, [3]=0.5}, uses=1000, maxlevel=3},
		}
	},
})

